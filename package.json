{
  "name": "lit-squircle",
  "version": "0.0.3",
  "keywords": [
    "lit",
    "web components",
    "squircle",
    "lit-elements"
  ],
  "type": "module",
  "main": "dist/squircle.js",
  "module": "dist/squircle.js",
  "types": "dist/squircle.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "analyze": "wca analyze src --outFile dist/custom-elements.json",
    "release": "npm run build && tsc -d && npm publish --access public"
  },
  "dependencies": {
    "figma-squircle": "^0.3.1",
    "lit": "^2.7.6"
  },
  "devDependencies": {
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "web-component-analyzer": "^1.1.7"
  }
}
